!!! 5
%html{:lang => "en"}
  %head
    %meta{:charset => "utf-8"}
      %title= "Tiramizoo - " + (yield(:title) || "Home")
      = stylesheet_link_tag "jquery.mobile-1.0a3pre", "jquery.jgrowl", "application"
      %link{"rel" => "apple-touch-icon", "href" => "images/apple-touch-icon.png"}
      = javascript_include_tag "http://pubnub-prod.appspot.com/js-api?pub-key=demo&sub-key=demo&origin=pubsub.pubnub.com&ssl=&java=&devmode=1"
      = javascript_include_tag "https://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js"
      = javascript_include_tag "jquery/jquery.jgrowl_compressed.js", "json2", "rails_mobile.js", "application", "jquery/jquery.mobile-1.0a3pre"
      = javascript_include_tag "http://maps.google.com/maps/api/js?sensor=true"
      = yield :meta
      = csrf_meta_tag
    %body
      = yield